<persistence xmlns="http://java.sun.com/xml/ns/persistence"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd"
version="2.0">
<persistence-unit name="persik">
    <provider>org.hibernate.jpa.HibernatePersistenceProvider</provider>
    <class>com.cookos.model.Subject</class>
    <class>com.cookos.model.Speciality</class>
    <class>com.cookos.model.Student</class>
    <class>com.cookos.model.User</class>
    <properties>       
        <property name="javax.persistence.jdbc.driver" value="com.mysql.cj.jdbc.Driver"/>       
        <property name="javax.persistence.jdbc.url"  value="jdbc:mysql://localhost/psp_cursach"/>       
        <property name="javax.persistence.jdbc.user" value="root"/>       
        <property name="javax.persistence.jdbc.password" value="12345678"/>       
    </properties>
</persistence-unit>
</persistence>
